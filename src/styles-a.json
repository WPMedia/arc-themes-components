{
	"attribution": {
		"styles": {
			"color": "var(--text-color)",
			"font-size": "var(--body-font-size-small)",
			"line-height": "var(--body-line-height-small)"
		}
	},
	"badge": {
		"styles": {
			"align-self": "baseline",
			"border-radius": "var(--border-radius-pill)",
			"color": "var(--global-white)",
			"font-size": "var(--body-font-size-tiny)",
			"font-weight": "var(--global-font-weight-7)",
			"line-height": "var(--body-line-height-tiny)",
			"padding": "var(--global-spacing-1) var(--global-spacing-2)"
		}
	},
	"badge-danger": {
		"styles": {
			"background-color": "var(--status-color-danger)"
		}
	},
	"badge-default": {
		"styles": {
			"background-color": "var(--text-color)"
		}
	},
	"badge-light": {
		"styles": {
			"background-color": "var(--text-color-subtle)"
		}
	},
	"badge-primary": {
		"styles": {
			"background-color": "var(--color-primary)"
		}
	},
	"badge-success": {
		"styles": {
			"background-color": "var(--status-color-success)"
		}
	},
	"badge-warning": {
		"styles": {
			"background-color": "var(--status-color-warning)"
		}
	},
	"body": {
		"styles": {
			"color": "var(--text-color)",
			"font-family": "var(--font-family-primary)",
			"font-size": "var(--body-font-size)",
			"line-height": "var(--body-line-height)"
		}
	},
	"button": {
		"styles": {
			"border-color": "transparent",
			"border-style": "var(--global-border-style-1)",
			"border-width": "var(--global-border-width-1)",
			"border-radius": "var(--border-radius)",
			"font-weight": "var(--global-font-weight-7)",
			"gap": "var(--global-spacing-2)"
		}
	},
	"button-small": {
		"styles": {
			"font-size": "var(--body-font-size-small)",
			"line-height": "var(--body-line-height-small)",
			"padding": "var(--global-spacing-1) var(--global-spacing-4)"
		}
	},
	"button-medium": {
		"styles": {
			"font-size": "var(--body-font-size-small)",
			"line-height": "var(--body-line-height-small)",
			"padding": "var(--global-spacing-3) var(--global-spacing-4)"
		}
	},
	"button-large": {
		"styles": {
			"font-size": "var(--body-font-size)",
			"line-height": "var(--body-line-height)",
			"padding": "var(--global-spacing-4)"
		}
	},
	"button-default": {
		"styles": {
			"background-color": "var(--global-white)",
			"border-color": "var(--global-neutral-7)",
			"border-width": "var(--global-border-width-1)",
			"color": "var(--global-neutral-7)"
		}
	},
	"button-primary": {
		"styles": {
			"background-color": "var(--color-primary)",
			"color": "var(--global-white)"
		}
	},
	"button-primary-hover": {
		"styles": {
			"background-color": "var(--global-neutral-7)"
		}
	},
	"button-primary-reverse": {
		"styles": {
			"background-color": "transparent",
			"border-color": "var(--color-primary)",
			"color": "var(--color-primary)"
		}
	},
	"button-primary-reverse-hover": {
		"styles": {
			"border-color": "var(--global-neutral-7)",
			"color": "var(--global-neutral-7)"
		}
	},
	"button-secondary": {
		"styles": {
			"background-color": "var(--global-white)",
			"color": "var (--global-black)"
		}
	},
	"button-secondary-hover": {
		"styles": {
			"background-color": "var(--global-neutral-7)",
			"color": "var(--global-white)"
		}
	},
	"button-secondary-reverse": {
		"styles": {
			"background-color": "transparent",
			"border-color": "var(--global-white)",
			"color": "var(--global-white)"
		}
	},
	"button-secondary-reverse-hover": {
		"styles": {
			"border-color": "var(--global-neutral-7)",
			"color": "var(--global-neutral-7)"
		}
	},
	"carousel": {
		"styles": {
			"--slides": 1
		}
	},
	"carousel-actions": {
		"styles": {
			"display": "none",
			"padding-left": "var(--global-spacing-4)",
			"padding-right": "var(--global-spacing-4)"
		}
	},
	"carousel-button": {
		"styles": {
			"box-shadow": "var(--global-box-shadow-1)",
			"border-radius": "var(--border-radius)",
			"background-color": "var(--global-white)",
			"height": "var(--global-spacing-8)",
			"width": "auto",
			"padding": "0 var(--global-spacing-2)",
			"display": "flex",
			"align-items": "center",
			"gap": "var(--global-spacing-2)"
		}
	},
	"carousel-button-previous": {
		"styles": {
			"background-color": "var(--global-white)"
		}
	},
	"carousel-button-next": {
		"styles": {
			"background-color": "var(--global-white)"
		}
	},
	"carousel-track": {
		"styles": {
			"gap": "var(--global-spacing-2)",
			"max-width": "var(--content-scale-width)",
			"width": "100%"
		}
	},
	"date": {
		"styles": {
			"font-size": "var(--body-font-size-small)",
			"line-height": "var(--body-line-height-small)"
		}
	},
	"divider": {
		"styles": {
			"background-color": "var(--border-color)",
			"height": "1px"
		}
	},
	"heading": {
		"styles": {
			"font-family": "var(--font-family-primary)",
			"font-size": "var(--heading-level-3-font-size)",
			"font-weight": "var(--heading-level-3-font-weight)",
			"line-height": "var(--heading-level-3-line-height)"
		}
	},
	"icon": {
		"styles": {
			"fill": "var(--icon-fill-color)",
			"height": "var(--global-spacing-5)",
			"width": "var(--global-spacing-5)"
		}
	},
	"image": {
		"styles": {
			"max-width": "100%"
		}
	},
	"input": {
		"styles": {
			"align-items": "flex-start",
			"display": "flex",
			"flex-direction": "column",
			"gap": "var(--global-spacing-1)"
		}
	},
	"input-input": {
		"styles": {
			"border": "var(--global-border-width-1) var(--global-border-style-1) var(--form-border-color)",
			"border-radius": "var(--border-radius)",
			"padding": "var(--global-spacing-4) var(--global-spacing-2)"
		}
	},
	"input-small-input": {
		"styles": {
			"padding": "var(--global-spacing-1) var(--global-spacing-2)"
		}
	},
	"input-input-focus": {
		"styles": {
			"outline": "solid 2px var(--form-border-color)",
			"outline-offset": "1px"
		}
	},
	"input-label": {
		"styles": {
			"font-weight": "var(--global-font-weight-7)"
		}
	},
	"input-error-input": {
		"styles": {
			"border-color": "var(--status-color-danger)"
		}
	},
	"input-error-input-focus": {
		"styles": {
			"outline-color": "var(--status-color-danger)"
		}
	},
	"input-success-input": {
		"styles": {
			"border-color": "var(--status-color-success)"
		}
	},
	"input-success-input-focus": {
		"styles": {
			"outline-color": "var(--status-color-success)"
		}
	},
	"input-warning-input": {
		"styles": {
			"border-color": "var(--status-color-warning)"
		}
	},
	"input-warning-input-focus": {
		"styles": {
			"outline-color": "var(--status-color-warning)"
		}
	},
	"link": {
		"styles": {
			"color": "var(--color-primary)"
		}
	},
	"link-active": {
		"styles": {
			"text-decoration": "none"
		}
	},
	"link-hover": {
		"styles": {
			"text-decoration": "none"
		}
	},
	"media-item": {
		"styles": {
			"align-items": "stretch",
			"display": "flex",
			"flex-direction": "column",
			"gap": "var(--global-spacing-2)"
		}
	},
	"media-item-fig-caption": {
		"styles": {
			"color": "var(--text-color-subtle)",
			"font-size": "var(--body-font-size-tiny)",
			"line-height": "var(--body-line-height-tiny)"
		}
	},
	"media-item-title": {
		"styles": {
			"font-weight": "var(--global-font-weight-7)"
		}
	},
	"overline": {
		"styles": {
			"color": "var(--text-color)",
			"font-size": "var(--heading-level-6-font-size)",
			"font-weight": "var(--heading-level-6-font-weight)",
			"line-height": "var(--heading-level-6-line-height)",
			"text-decoration": "none"
		}
	},
	"paragraph": {
		"styles": {
			"font-family": "var(--font-family-secondary)",
			"font-size": "var(--body-font-size)",
			"font-weight": "var(--body-font-weight)",
			"line-height": "var(--body-line-height)"
		}
	},
	"pill": {
		"styles": {
			"background-color": "var(--color-primary)",
			"border-radius": "var(--border-radius-pill)",
			"color": "var(--global-white)",
			"font-size": "var(--body-font-size-tiny)",
			"padding": "var(--global-spacing-1) var(--global-spacing-4)",
			"text-decoration": "none"
		}
	},
	"pill-hover": {
		"styles": {
			"background-color": "var(--global-neutral-7)"
		}
	},
	"stack-divider-vertical": {
		"styles": {
			"border-bottom": "var(--global-border-width-1) var(--global-border-style-1) var(--border-color)"
		}
	},
	"stack-divider-horizontal": {
		"styles": {
			"border": "none",
			"border-left": "var(--global-border-width-1) var(--global-border-style-1) var(--border-color)",
			"width": "1px",
			"height": "auto"
		}
	},
	"video": {
		"styles": {
			"margin": "auto"
		}
	},
	"video-frame": {
		"styles": {
			"background-color": "var(--global-black)"
		}
	}
}
