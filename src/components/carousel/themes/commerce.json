{
	"carousel": {
		"styles": {
			"default": {
				"--slides": 1
			},
			"desktop": {
				"--slides": 4
			}
		}
	},
	"carousel-actions": {
		"styles": {
			"default": {
				"display": "none",
				"padding-inline-start": "var(--global-spacing-4)",
				"padding-inline-end": "var(--global-spacing-4)"
			},
			"desktop": {
				"display": "flex"
			}
		}
	},
	"carousel-button": {
		"styles": {
			"default": {
				"box-shadow": "var(--global-box-shadow-1)",
				"border-radius": "var(--border-radius)",
				"background-color": "var(--global-white)",
				"block-size": "var(--global-spacing-8)",
				"inline-size": "auto",
				"padding-block-start": "0",
				"padding-inline-start": "var(--global-spacing-2)",
				"padding-block-end": "0",
				"padding-inline-end": "var(--global-spacing-2)",
				"display": "flex",
				"align-items": "center",
				"gap": "var(--global-spacing-2)"
			}
		}
	},
	"carousel-track": {
		"styles": {
			"default": {
				"gap": "var(--global-spacing-2)",
				"max-inline-size": "var(--content-scale-width)",
				"inline-size": "100%"
			},
			"desktop": {
				"gap": "var(--global-spacing-6)"
			}
		}
	},
	"carousel-indicator-dots-container": {
		"styles": {
			"default": {
				"display": "flex",
				"gap": "var(--global-spacing-4)",
				"padding-block-start": "var(--global-spacing-4)",
				"padding-inline-start": "0",
				"padding-block-end": "0",
				"padding-inline-end": "0",
				"justify-content": "center",
				"align-items": "center"
			}
		}
	},
	"carousel-indicator-dot": {
		"styles": {
			"default": {
				"block-size": "var(--global-spacing-3)",
				"inline-size": "var(--global-spacing-3)",
				"border-block-start-width": "var(--global-border-width-1)",
				"border-block-end-width": "var(--global-border-width-1)",
				"border-inline-start-width": "var(--global-border-width-1)",
				"border-inline-end-width": "var(--global-border-width-1)",
				"border-block-start-style": "var(--global-border-style-1)",
				"border-block-end-style": "var(--global-border-style-1)",
				"border-inline-start-style": "var(--global-border-style-1)",
				"border-inline-end-style": "var(--global-border-style-1)",
				"border-block-start-color": "var(--border-color)",
				"border-block-end-color": "var(--border-color)",
				"border-inline-start-color": "var(--border-color)",
				"border-inline-end-color": "var(--border-color)",
				"border-radius": "var(--border-radius-circle)",
				"background-color": "transparent"
			}
		}
	},
	"carousel-indicator-dot-active": {
		"styles": {
			"default": {
				"border": "none",
				"block-size": "var(--global-spacing-4)",
				"inline-size": "var(--global-spacing-4)",
				"background-color": "var(--global-neutral-4)"
			}
		}
	},
	"carousel-indicator-thumbnails-container": {
		"styles": {
			"default": {
				"display": "block",
				"padding-block-start": "var(--global-spacing-4)",
				"padding-inline-start": "var(--global-spacing-3)",
				"padding-block-end": "0",
				"padding-inline-end": "var(--global-spacing-3)",
				"inline-size": "100%",
				"overflow-x": "auto",
				"overflow-y": "hidden",
				"overflow-inline": "auto",
				"overflow-block": "hidden",
				"block-size": "var(--global-spacing-11)",
				"white-space": "nowrap"
			}
		}
	},
	"carousel-indicator-thumbnail": {
		"styles": {
			"default": {
				"margin-block-start": "0",
				"margin-inline-start": "var(--global-spacing-3)",
				"margin-block-end": "0",
				"margin-inline-end": "var(--global-spacing-3)",
				"block-size": "var(--global-spacing-8)",
				"inline-size": "var(--global-spacing-8)",
				"background-color": "transparent",
				"opacity": "40%",
				"border-block-start-width": "var(--global-border-width-1)",
				"border-block-end-width": "var(--global-border-width-1)",
				"border-inline-start-width": "var(--global-border-width-1)",
				"border-inline-end-width": "var(--global-border-width-1)",
				"border-block-start-style": "var(--global-border-style-1)",
				"border-block-end-style": "var(--global-border-style-1)",
				"border-inline-start-style": "var(--global-border-style-1)",
				"border-inline-end-style": "var(--global-border-style-1)",
				"border-block-start-color": "transparent",
				"border-block-end-color": "transparent",
				"border-inline-start-color": "transparent",
				"border-inline-end-color": "transparent"
			}
		}
	},
	"carousel-indicator-thumbnail-active": {
		"styles": {
			"default": {
				"border-block-start-width": "var(--global-border-width-1)",
				"border-block-end-width": "var(--global-border-width-1)",
				"border-inline-start-width": "var(--global-border-width-1)",
				"border-inline-end-width": "var(--global-border-width-1)",
				"border-block-start-style": "var(--global-border-style-1)",
				"border-block-end-style": "var(--global-border-style-1)",
				"border-inline-start-style": "var(--global-border-style-1)",
				"border-inline-end-style": "var(--global-border-style-1)",
				"border-block-start-color": "var(--global-black)",
				"border-block-end-color": "var(--global-black)",
				"border-inline-start-color": "var(--global-black)",
				"border-inline-end-color": "var(--global-black)",
				"opacity": "1"
			}
		}
	}
}
